{
  "name": "@nofrixion/ui",
  "version": "0.0.0",
  "license": "MIT",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist",
    "./styles.css": "./dist/index.css",
    "./atoms": {
      "import": "./dist/atoms/index.mjs",
      "require": "./dist/atoms/index.js"
    },
    "./molecules": {
      "import": "./dist/molecules/index.mjs",
      "require": "./dist/molecules/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ],
      "atoms": [
        "./dist/atoms/index.d.ts"
      ],
      "molecules": [
        "./dist/molecules/index.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "tsup src/index.tsx src/atoms/index.ts src/molecules/index.ts --watch --format esm,cjs --dts --external react",
    "build": "tsup src/index.tsx src/atoms/index.ts src/molecules/index.ts --format esm,cjs --dts --external react",
    "lint": "eslint ."
  },
  "dependencies": {
    "@radix-ui/react-select": "^1.2.2",
    "framer-motion": "^10.15.0"
  },
  "devDependencies": {
    "@nofrixion/eslint-config": "workspace:^",
    "@nofrixion/tailwind-config": "workspace:^",
    "@nofrixion/tsconfig": "workspace:^",
    "@nofrixion/utils": "workspace:^",
    "@types/react": "^18.2.18",
    "autoprefixer": "^10.4.14",
    "class-variance-authority": "^0.7.0",
    "eslint": "^8.46.0",
    "postcss": "^8.4.27",
    "react": "^18.2.0",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6"
  }
}
