{
  "name": "@nofrixion/ui",
  "version": "0.0.0",
  "license": "MIT",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist",
    "./styles.css": "./dist/index.css",
    "./atoms": {
      "import": "./dist/atoms/index.mjs",
      "require": "./dist/atoms/index.js"
    },
    "./molecules": {
      "import": "./dist/molecules/index.mjs",
      "require": "./dist/molecules/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ],
      "atoms": [
        "./dist/atoms/index.d.ts"
      ],
      "molecules": [
        "./dist/molecules/index.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "tsup src/index.tsx src/atoms/index.ts src/molecules/index.ts --watch --format esm,cjs --dts --external react",
    "build": "tsup src/index.tsx src/atoms/index.ts src/molecules/index.ts --format esm,cjs --dts --external react",
    "lint": "eslint ."
  },
  "dependencies": {
    "@headlessui/react": "^1.7.16",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-scroll-area": "^1.0.4",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.6",
    "date-fns": "^2.30.0",
    "downshift": "^8.1.0",
    "framer-motion": "^10.15.0",
    "lodash": "^4.17.21",
    "react-multi-date-picker": "^4.1.2",
    "react-qr-code": "^2.0.11",
    "react-text-mask": "^5.5.0",
    "react-toastify": "^9.1.3",
    "react-use-measure": "^2.1.1",
    "text-mask-addons": "^3.8.0",
    "usehooks-ts": "^2.9.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nofrixion/eslint-config": "workspace:^",
    "@nofrixion/tailwind-config": "workspace:^",
    "@nofrixion/tsconfig": "workspace:^",
    "@nofrixion/utils": "workspace:^",
    "@types/lodash": "^4.14.196",
    "@types/react": "^18.2.18",
    "@types/react-text-mask": "^5.4.11",
    "@types/uuid": "^9.0.2",
    "autoprefixer": "^10.4.14",
    "class-variance-authority": "^0.7.0",
    "eslint": "^8.46.0",
    "postcss": "^8.4.27",
    "react": "^18.2.0",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6"
  }
}
