{
  "name": "@nofrixion/utils",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "./types": {
      "import": "./dist/types/index.mjs",
      "require": "./dist/types/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ],
      "types": [
        "./dist/types/index.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "tsup index.ts types/index.ts --watch --format esm,cjs --dts",
    "build": "tsup index.ts types/index.ts --format esm,cjs --dts",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@nofrixion/eslint-config": "workspace:^",
    "@nofrixion/tailwind-config": "workspace:^",
    "@nofrixion/tsconfig": "workspace:^",
    "clsx": "^2.0.0",
    "tailwind-merge": "^1.14.0",
    "tsup": "^7.2.0"
  },
  "dependencies": {
    "date-fns": "^2.30.0"
  }
}
